<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
  
  <grid model="com.axelor.gst.db.ProductCategory"
    title="Product Category" name="productCategory-grid">
     <field name="name" />
     <field name="code" />
     <field name="gstRate" />  
  </grid>
  
  <form model="com.axelor.gst.db.ProductCategory"
    title="Product Category" name="productCategory-form">
    <panel>
     <field name="name" />
     <field name="code" />
     <field name="gstRate" />
    </panel>  
  </form>
</object-views>