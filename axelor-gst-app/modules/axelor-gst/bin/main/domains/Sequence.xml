<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="gst" package="com.axelor.gst.db"/>
  
  
  <entity name="Sequence" sequential="true" cachable="true">
    <string name="prefix" required="true"/>
    <string name="suffix" />
    <integer name="padding" min="1" max="10"/>
    <string name="nextNumber" namecolumn="true"/>
    <many-to-one name="model" ref="com.axelor.meta.db.MetaModel" />
  </entity>
</domain-models>
